/* EMPLOYEE PORTAL */
.employee-wrapper .left.image-float {
	width:100%;
	max-width: 237px;
	margin:0 auto;
	float:none;
}
.employee-wrapper .right.employee-service {
	float:none;
	text-align: center;
}
.right.employee-service h3 {
	text-align: center;
}
.employee-service p {
	margin:0;
}

@media screen and (min-width: 600px) {
	.employee-wrapper .left.image-float {
		width:25%;
		max-width: 237px;
		float:left;
		margin: 0 5% 1em 0;	
	}
	.employee-wrapper .right.employee-service {
		width:70%;
		float:right;
		text-align: left;
	}	
	.right.employee-service h3 {
		text-align: left;
	}	
}


